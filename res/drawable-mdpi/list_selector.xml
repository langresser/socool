<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#34ffcc33" />
        </shape>
    </item>
    <item android:state_pressed="false" android:drawable="@color/transparent" />
    <item android:state_focused="true" android:drawable="@color/transparent" />
    <item android:state_focused="false" android:drawable="@color/transparent" />
    <item android:drawable="@color/transparent" />
</selector>
